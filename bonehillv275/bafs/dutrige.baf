IF
  Global("EAST_BLOCK","GLOBAL",0)
  OpenState(Myself,TRUE)
THEN
  RESPONSE #100
    SetGlobal("EAST_BLOCK","GLOBAL",1)
END

IF
  !Global("EAST_BLOCK","GLOBAL",0)
  OpenState(Myself,FALSE)
THEN
  RESPONSE #100
    Unlock(Myself)
    OpenDoor(Myself)
    Lock(Myself)
END

IF
  !Global("EAST_BLOCK","GLOBAL",0)
  OpenState(Myself,TRUE)
THEN
  RESPONSE #100
    Lock(Myself)
    Deactivate(Myself)
END

